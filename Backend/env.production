# Production Environment Variables for Railway
NODE_ENV=production
PORT=5000

# MongoDB - Use Railway's MongoDB plugin or your own MongoDB Atlas
CONN_STRING=your_mongodb_atlas_connection_string_here

# Clerk Authentication
CLERK_SECRET_KEY=your_clerk_secret_key_here

# AI Services
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Cloudinary (Already configured)
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Other APIs
SPOONACULAR_API_KEY=your_spoonacular_api_key_here
INFERMEDICA_APP_ID=your_infermedica_app_id_here
INFERMEDICA_APP_KEY=your_infermedica_app_key_here

# Email (if using)
EMAIL_USER=your_email_here
EMAIL_PASS=your_email_password_here
